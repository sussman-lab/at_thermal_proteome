	month = jul,
	year = {2006},
	pmid = {16606683},
	pages = {1600--1607},
	file = {Full Text PDF:/home/jeremy/.zotero/storage/RHQZPMIN/Alexa et al. - 2006 - Improved scoring of functional groups from gene ex.pdf:application/pdf;Full Text PDF:/home/jeremy/.zotero/storage/QVV49A7U/Alexa et al. - 2006 - Improved scoring of functional groups from gene ex.pdf:application/pdf;Snapshot:/home/jeremy/.zotero/storage/QNZ2AWTW/btl140.html:text/html}
