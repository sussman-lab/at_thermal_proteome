	journal = {Bioinformatics},
	author = {Montanucci, Ludovica and Fariselli, Piero and Martelli, Pier Luigi and Casadio, Rita},
	month = jul,
	year = {2008},
	pmid = {18586713},
	pages = {i190--i195},
	file = {Full Text PDF:/home/jeremy/.zotero/storage/UN6AIH26/Montanucci et al. - 2008 - Predicting protein thermostability changes from se.pdf:application/pdf}
}

